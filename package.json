{
  "name": "trzy",
  "version": "0.2.0",
  "description": "A utility library for Three.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "three",
    "three.js",
    "util",
    "r3f",
    "threlte"
  ],
  "main": "./dist/trzy.umd.js",
  "module": "./dist/trzy.js",
  "types": "./dist/src/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/trzy.js",
      "require": "./dist/trzy.umd.js"
    }
  },
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "check": "tsc --noEmit",
    "lint": "eslint ./src",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "models": "node ./scripts/models.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/trzy.git"
  },
  "author": "Micheal Parks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michealparks/trzy/issues"
  },
  "homepage": "https://github.com/michealparks/trzy#readme",
  "peerDependencies": {
    "three": "*"
  },
  "devDependencies": {
    "@storybook/addon-actions": "v7.1.0",
    "@storybook/addon-docs": "^7.1.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "v7.1.0",
    "@storybook/addon-links": "v7.1.0",
    "@storybook/html": "v7.1.0",
    "@storybook/html-vite": "v7.1.0",
    "@types/three": "^0.154.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-unicorn": "^48.0.0",
    "postprocessing": "^6.32.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simplex-noise": "^4.0.1",
    "storybook": "^7.1.0",
    "three": "^0.154.0",
    "three-inspect": "^0.3.4",
    "three-mesh-bvh": "^0.6.2",
    "trzy": "^0.1.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vite-plugin-glsl": "^1.1.2"
  }
}
