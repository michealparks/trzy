{
	"name": "trzy",
	"version": "0.3.3",
	"description": "A utility library for Three.js",
	"type": "module",
	"author": "Micheal Parks",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/michealparks/trzy/issues"
	},
	"homepage": "https://github.com/michealparks/trzy#readme",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"keywords": [
		"three",
		"three.js",
		"util",
		"3d",
		"webgl"
	],
	"scripts": {
		"lint": "eslint ./src",
		"models": "node ./scripts/models.js",
		"start": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"three": "*"
	},
	"dependencies": {
		"simplex-noise": "^4.0.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.22.4",
		"@sveltejs/package": "^2.2.0",
		"@types/prismjs": "^1.26.0",
		"@types/three": "^0.155.0",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.46.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"postcss": "^8.4.27",
		"postprocessing": "^6.32.2",
		"prismjs": "^1.29.0",
		"publint": "^0.2.0",
		"simplex-noise": "^4.0.1",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"tailwindcss": "^3.3.3",
		"three": "^0.155.0",
		"three-inspect": "^0.3.4",
		"three-mesh-bvh": "^0.6.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.8",
		"vite-plugin-glsl": "^1.1.2"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
